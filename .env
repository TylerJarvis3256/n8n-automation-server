# Replace <directory-path> with the path where you created folders earlier
DATA_FOLDER=/home/tylerjarvis8/n8n-docker-caddy

# The top level domain to serve from, this should be the same as the subdomain you created above
DOMAIN_NAME=risingsunseo.com

# The subdomain to serve from
SUBDOMAIN=n8n

# DOMAIN_NAME and SUBDOMAIN combined decide where n8n will be reachable from
# above example would result in: https://n8n.example.com

# Optional timezone to set which gets used by Cron-Node by default
# If not set New York time will be used
GENERIC_TIMEZONE=America/Phoenix

# The email address to use for the SSL certificate creation
SSL_EMAIL=tylerjarvis3256@gmail.com

# Basic Authentication - keeps my n8n instance private
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=tylerjarvis8
N8N_BASIC_AUTH_PASSWORD=Jarvboy3256#

# Database settings for production reliability
DB_TYPE=postgresdb
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=Jarvboy3256#

# Encryption for stored credentials (API keys, passwords)
N8N_ENCRYPTION_KEY=234878e7235352873e8587107b78d77a

# Full URL for webhook triggers
WEBHOOK_URL=https://n8n.risingsunseo.com/

# === PRODUCTION SETTINGS ===
NODE_ENV=production
N8N_LOG_LEVEL=info
N8N_METRICS=false

# === EXECUTION SETTINGS ===
# Important for handling 100+ daily jobs
EXECUTIONS_PROCESS=main
EXECUTIONS_TIMEOUT=300
EXECUTIONS_TIMEOUT_MAX=3600

# === API RATE LIMITING ===
# Prevents overwhelming job board APIs
N8N_PAYLOAD_SIZE_MAX=16
N8N_METRICS_PREFIX=n8n_
