=== CORE CONFIGURATION ===
DATA_FOLDER=/home/username/n8n-docker-caddy
DOMAIN_NAME=yourdomain.com
SUBDOMAIN=n8n
SSL_EMAIL=your-email@example.com
GENERIC_TIMEZONE=America/Phoenix
=== SECURITY ===
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your-username
N8N_BASIC_AUTH_PASSWORD=generate-secure-password
=== DATABASE ===
DB_TYPE=postgresdb
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=generate-secure-password
=== N8N CONFIGURATION ===
N8N_ENCRYPTION_KEY=generate-32-char-hex-string
WEBHOOK_URL=https://n8n.yourdomain.com/
NODE_ENV=production
N8N_LOG_LEVEL=info
